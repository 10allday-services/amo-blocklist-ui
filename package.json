{
  "name": "amo-blocklist-ui",
  "version": "0.0.1",
  "description": "AMO blocklist UI.",
  "scripts": {
    "build": "NODE_ENV=production node_modules/.bin/webpack --optimize-minimize --config webpack.prod.js && cp index.html style.css build/",
    "publish": "npm run build && node_modules/.bin/gh-pages --dist build/",
    "start": "node_modules/.bin/webpack-dev-server --config webpack.dev.js --port=3000",
    "test": "node_modules/.bin/mocha --recursive --compilers js:babel/register --require ./test/setup-jsdom.js test/"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "eventemitter2": "^0.4.14",
    "jsonschema": "^1.0.2",
    "kinto": "^1.1.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "babel": "^5.8.20",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "css-loader": "^0.15.6",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^3.6.3",
    "gh-pages": "^0.4.0",
    "jsdom": "^6.5.1",
    "mocha": "^2.2.5",
    "react-hot-loader": "^1.2.8",
    "sinon": "git://github.com/uberVU/Sinon.JS.git#0aaf834c9f",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozilla-services/amo-blocklist-ui.git"
  },
  "keywords": [
    "amo",
    "react",
    "blocklist",
    "json-schema"
  ],
  "author": "Nicolas Perriault <nperriault@mozilla.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mozilla-services/amo-blocklist-ui/issues"
  },
  "homepage": "https://github.com/mozilla-services/amo-blocklist-ui#readme"
}
